@model List<DataModel>
@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h3 class="display-4">View Data</h3>
    @if (Model != null && Model.Count > 0)
    {
        <table class="table table-bordered table-striped table-sm">
            <thead>
            <tr>
                <th>Id</th>
                <th>Node Id</th>
                <th>Login</th>
                <th>Hooks Url</th>
            </tr>
            </thead>
            <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.id)</td>
                    <td>@Html.DisplayFor(modelItem => item.node_id)</td>
                    <td>@Html.DisplayFor(modelItem => item.login)</td>
                    <td>@Html.DisplayFor(modelItem => item.hooks_url)</td>
                </tr>
            }
            </tbody>
        </table>
    }
    
</div>
